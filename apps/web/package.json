{
  "name": "web",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port 3001",
    "build": "bunx --bun next build",
    "start": "bunx --bun next start",
    "fmt": "biome format ./src/**/*.{ts,tsx} --no-errors-on-unmatched",
    "fmt:fix": "biome format --write ./src/**/*.{ts,tsx} --no-errors-on-unmatched",
    "lint": "biome lint ./src/**/*.{ts,tsx} --error-on-warnings --no-errors-on-unmatched",
    "lint:fix": "biome lint --apply ./src/**/*.{ts,tsx} --no-errors-on-unmatched",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@elysiajs/eden": "^1.2.0",
    "@hookform/resolvers": "^3.9.1",
    "@tanstack/react-query": "^5.50.0",
    "@tanstack/react-table": "^8.20.5",
    "api": "workspace:*",
    "better-auth": "^1.2.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "framer-motion": "^12.9.2",
    "lucide-react": "^0.460.0",
    "next": "^15.2.1",
    "next-themes": "^0.4.4",
    "nuqs": "^2.2.3",
    "plaiceholder": "^3.0.0",
    "react-hook-form": "^7.53.2",
    "server-only": "^0.0.1",
    "shared": "workspace:*",
    "sonner": "^1.7.0",
    "ui": "workspace:*",
    "utils": "workspace:*"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "@repo/typescript-config": "*",
    "@types/node": "^22.13.9",
    "typescript": "5.8.2"
  }
}
